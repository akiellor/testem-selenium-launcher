{
  "framework": "mocha",
  "src_files": [
    "tests/*_spec.js"
  ],
  "launchers": {
    "Chrome_iPhone7-portrait": {
      "exe": "node_modules/.bin/testem-selenium-launcher",
      "args": [
        "<url>",
        "http://localhost:9515",
        {
          "browserName": "chrome",
          "chromeOptions": {
            "args": [
              "--test-type",
              "--window-size=414,736"
            ]
          }
        }
      ],
      "protocol": "browser"
    }
  }
}
